{%comment%}
{% assign shappify_min = shappify-min-price.price_max %}
{% for variant in shappify-min-price.variants %}
    {% if variant.metafields.shappify_csp.csp_isparent == "1" and variant.metafields.shappify_csp.csp_tag == tag %}
        {% if variant.price < shappify_min %}
            {% assign shappify_min = variant.price %}
        {% endif %}
    {% endif %}
{% endfor %}

{%comment%}gc--[{%endcomment%}{%- capture geolizr_prepend_code -%}{%- render 'geolizr-currency', geolizr_price: shappify_min -%}{% endcapture %}{%comment%}]--gc{%endcomment%}{{ shappify_min | money | prepend: geolizr_prepend_code | append: "</span>"}}
{%endcomment%}